    "express": "~4.18.2",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "kill-port": "^1.6.0",
    "mongoose": "^5.9.18",
    "morgan": "~1.9.1",
    "react-color": "^2.18.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
